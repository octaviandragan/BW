{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Debug Playwright Test",
        "type": "node",
        "request": "launch",
        "skipFiles": ["<node_internals>/**"],
        "program": "${workspaceFolder}/node_modules/.bin/playwright",
        "args": ["test"],
        "outFiles": ["${workspaceFolder}/dist/**/*.js"],
        "preLaunchTask": "tsc: build - tsconfig.json"
      }
    ]
  }
  